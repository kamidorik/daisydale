function H2CA(hex) {
 var _loc3_ = [];
 var _loc1_ = hex.substr(0, 2) != "0x" ? 0 : 2;
 while (_loc1_ < hex.length) {
  _loc3_.push(parseInt(hex.substr(_loc1_, 2), 16));
  _loc1_ = _loc1_ + 2;
 }
 return _loc3_;
}
